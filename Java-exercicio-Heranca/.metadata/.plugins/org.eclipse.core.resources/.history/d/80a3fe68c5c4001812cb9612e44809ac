package exercicio;

public class Principal {
	public static void main(String[] args) {
		Aluno aluno = new Aluno();
		aluno.setNome("Carlos");
		aluno.setCpf("098.546.669-79");
		aluno.setRg("87.485.197");
		Endereco endereco = new Endereco();
		endereco.setRua("Catanduva");
		endereco.setNumero(210);
		endereco.setBairro("Jardim Molivera");
		endereco.setCidade("Douradina");
		endereco.setEstado("PR");
		endereco.setPais("Brasil");
		aluno.setMatricula(01);
		aluno.setCurso("");
		System.out.println("Dados do Aluno: \n \n" + aluno);
	}
}
