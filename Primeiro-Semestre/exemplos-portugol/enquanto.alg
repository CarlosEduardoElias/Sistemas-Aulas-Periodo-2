algoritmo "enquanto"

var
   valor1, valor2 : inteiro
inicio
   valor1 <- 0
   valor2 <- 0
   
   enquanto (valor1 <10) faca
      escreval("Valor 1", valor1)
      valor1 <- valor1 + 1
   fimenquanto
   
   escreval ("==================================")
   
   enquanto (valor2 < valor1) faca
      escreval("Valor 2", valor2)
      valor2 <- valor2 + 1
   fimenquanto

fimalgoritmo
