algoritmo "vetor_exemplo1"

var
   vetorInteiro : vetor[0..1] de inteiro
   vetorCaractere : vetor[0..1] de caractere
inicio
   vetorInteiro[0] <- 10
   vetorInteiro[1] <- 15
   vetorCaractere[0] <- "Carlos"
   vetorCaractere[1] <- "Eduardo"
   
   escreval("Vetor Inteiro na Posição 0: ", vetorInteiro[0])
   escreval("Vetor de Caractere na Posição 1: ", vetorCaractere[1])
   
fimalgoritmo
