package exemplos;

public class Competicao {
	public static void main(String[]args) {
		Ciclista c = new Ciclista(null, null, 0);
		c.ciclista("nome");
		c.equipe("doura");
		c.numero(0);
		System.out.println(c);
		Bike b = new Bike(null, null, null, 0);
		b.tipo("Moutain Bike");
		b.modelo("");
		b.quadro("");
		b.aro();
		System.out.println(b);
	}

}


