package exemplos;

public class Ciclista {
	private String ciclista;
	private String equipe;
	private int numero;
	

public Ciclista (String ciclista, String equipe, int numero) {
	this.ciclista = ciclista;
	this.equipe = equipe;
	this.numero = numero;
}

public void ciclista(String ciclista) {
		this.ciclista = ciclista;
}

public void equipe(String equipe) {
		this.equipe = equipe;
}
	
public void numero(int numero) {
			this.numero = numero;
}


public String toString() {
	return "Nome: " + ciclista;
}

public String String() {
	return "Equipe: " + equipe;
}

public int numero() {
	return numero;
}

}