package exemplos;

public class Competicao {
	public static void main(String[]args) {
		Ciclista c = new Ciclista(null, null, 0);
		c.ciclista("Cadu");
		c.equipe("Dourabikers");
		c.numero(18);
		System.out.println(c);
		Bike b = new Bike(null, null, null, 0);
		b.tipo("MoutainBike");
		b.modelo("WendyBike");
		b.quadro("19");
		System.out.println(b);
	}

}


